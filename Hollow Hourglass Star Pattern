a=5
for i in range(1,a)  :
  for j in range(i,a):
     print("*",end=" ")
  for k in range(1,i):
    print(" ",end=" ")
  for l in range(1,i):
    print(" ",end=" ")
  for m in range(i,a):
    print("*",end=" ")
  print()
for r in range (1,a):
  for s in range (1,r+1):
    print("*",end=" ")
  for t in range (r,a-1):
    print(" ",end = " ")
  for u in range (r,a-1):
    print(" ",end = " ")
  for v in range (1,r+1):
    print ("*",end=" ")
  print()
